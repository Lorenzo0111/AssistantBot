{
  "name": "assistant-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "ts-node-esm src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm src/main.ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.3",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "6.14.0",
    "axios": "^1.11.0",
    "discord.js": "^14.21.0",
    "discordx": "^11.12.5",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma"
    ]
  }
}
